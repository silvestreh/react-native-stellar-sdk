{
  "name": "expo-stellar-sdk",
  "version": "0.0.0",
  "description": "Stellar SDK for Expo",
  "author": "Ian McGregor <ian@mcgregor.codes>",
  "contributors": ["Silvestre Herrera <silvestre.herrera@gmail.com>"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/silvestreh/expo-stellar-sdk"
  },
  "main": "stellar-sdk.js",
  "scripts": {
    "build": "webpack",
    "test": "jest",
    "lint": "standard --fix",
    "publish": "git push origin && git push origin --tags",
    "release:patch": "npm version patch && npm run publish",
    "release:minor": "npm version minor && npm run publish",
    "release:major": "npm version major && npm run publish"
  },
  "dependencies": {
    "expo-random": "^8.1.0",
    "react-native-randombytes": "3.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "create-hash": "1.2.0",
    "jest": "25.3.0",
    "metro-react-native-babel-preset": "0.59.0",
    "react-native": "0.62.2",
    "standard": "^14.3.3",
    "stellar-sdk": "^5.0.3",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "verbose": true,
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "expect",
      "it",
      "beforeEach",
      "afterEach"
    ],
    "ignore": [
      "stellar-sdk.js"
    ]
  }
}
